local Services = {
	Players = game:GetService('Players'),
	RunService = game:GetService('RunService'),
	CoreGui = game:GetService('CoreGui'),
	Workspace = game:GetService('Workspace'),
	ReplicatedStorage = game:GetService('ReplicatedStorage'),
	UserService = game:GetService('UserService')
}
local SafeGuis = {
	'BubbleChat',
	'CatalogScreenGui',
	'Chat',
	'DeathScreenGui',
	'ProximityPrompts',
	'SidePanelScript'
}
local OthersTable = {}
local Insert = Services.ReplicatedStorage.Remotes.ToggleAsset
local LP = Services.Players.LocalPlayer --Your Player
local Damage = math.huge --InfiniteDamage
local DiamondID = 173755801 --DiamondBladeID
local DiamondName = 'Diamond Blade Sword' --DiamondBladeName
local RocketID = 169602103 --RocketJumperID
local RocketName = 'RocketJumper' --RocketJumberName
if not _G.Targets then
	_G.Targets = {}
end

if not _G.Filter then
	_G.Filter = 'Display'
end

local function FireRemote(Path, ...)
	if Path == nil then
		return error("Remote Doesn't Exist")
	end
	local Args = {...}
	if Path:IsA('RemoteFunction') then
		Path:InvokeServer(unpack(Args))
	elseif Path:IsA('RemoteEvent') then
		Path:FireServer(unpack(Args))
	end
end
local function DiamondFunction(Target,Damage)
	pcall(function()
		if LP.Backpack:FindFirstChild(DiamondName) then
			Path = LP.Backpack:FindFirstChild(DiamondName).GearScript.Controller
		elseif LP.Character:FindFirstChild(DiamondName) then
			Path = LP.Character:FindFirstChild(DiamondName).GearScript.Controller
		end
		FireRemote(Path,7,Target,Damage)
	end)
end
local function ToggleGear(ID)
	Insert:InvokeServer(ID)
end
local function InsertGear(ToolId, ToolName)
	if not LP.Backpack:FindFirstChild(ToolName) and not LP.Character:FindFirstChild(ToolName) then
		ToggleGear(ToolId)
	end
end
local function TargetPlayer(player, button) --TargetsPlayers
		if table.find(_G.Targets, player.UserId) then
			local Index = table.find(_G.Targets, player.UserId)
			table.remove(_G.Targets, Index)
			button.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
		else
			table.insert(_G.Targets, player.UserId)
			button.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
		end
end
local function PlayerAdded(Player) --FiresWhenPlayerAdded
	local PlayerButton = Instance.new('TextButton')
	local RoundCorner = Instance.new("UICorner")
	local IDValue = Instance.new('NumberValue')
	if table.find(_G.Targets, Player.UserId) then
		PlayerButton.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
	else
		PlayerButton.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
	end
	if _G.Filter == 'Display' then
		PlayerButton.Name = Player.DisplayName
		PlayerButton.Text = Player.DisplayName
	else
		PlayerButton.Name = Player.Name
		PlayerButton.Text = Player.Name
	end
	RoundCorner.Parent = PlayerButton
	IDValue.Value = Player.UserId
	IDValue.Name = Player.UserId
	IDValue.Parent = PlayerButton
	PlayerButton.AnchorPoint = Vector2.new(0.5, 0.5)
	PlayerButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	PlayerButton.BorderSizePixel = 0
	PlayerButton.Position = UDim2.new(0.254883647, 0, 0.866034269, 0)
	PlayerButton.Size = UDim2.new(0, 129, 0, 20)
	PlayerButton.FontFace = Font.fromName('JosefinSans', Enum.FontWeight.SemiBold)
	PlayerButton.TextColor3 = Color3.fromRGB(130, 167, 255)
	PlayerButton.TextSize = 14.000
	PlayerButton.TextWrapped = true
	PlayerButton.Parent = TargetsFrame
	PlayerButton.MouseButton1Click:Connect(function()
		TargetPlayer(Player, PlayerButton)
	end)
end
local function PlayerRemoved(Player)
	if TargetsFrame:FindFirstChild(Player.Name) then
		TargetsFrame[Player.Name]:Destroy()
	end
end
local function RocketFunction(Angle,Position)
    pcall(function()
        if LP.Backpack:FindFirstChild('RocketJumper') then
            Path = LP.Backpack:FindFirstChild('RocketJumper').FireRocket
        elseif LP.Character:FindFirstChild('RocketJumper') then
            Path = LP.Character:FindFirstChild('RocketJumper').FireRocket
        end
        FireRemote(Path,Angle,Position)
    end)
end
local function CBringFunction(Char)
    pcall(function()
        if LP.Character and LP.Character:FindFirstChildOfClass('Tool') and LP.Character:FindFirstChildOfClass("Tool").Handle then
            for i,v in pairs(Char:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.Anchored = true
                    v.Massless = true
                    v.CanCollide = false
                    v.CFrame = LP.Character:FindFirstChildOfClass("Tool").Handle.CFrame * CFrame.new(Vector3.new(0, 0, 0))
                elseif v:IsA("Tool") or v:IsA("Motor6D") or v:IsA("Attachment") then
                    v:Remove()
                end
            end
        else
            return warn("ERROR('No Tool'")
        end
    end)
end

for i,player in ipairs(game.Players:GetPlayers()) do
	if player ~= LP then
		PlayerAdded(player)
	end
end

Services.Players.PlayerAdded:Connect(PlayerAdded)
Services.Players.PlayerRemoving:Connect(PlayerRemoved)

if _G.Filter == 'Display' then
	Username.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
	DisplayName.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
	for i,v in pairs(TargetsFrame:GetChildren()) do
		if v:IsA('TextButton') then
			PlayerId = v:FindFirstChildOfClass('NumberValue').Value
			local Success, UserData = pcall(function() return Services.UserService:GetUserInfosByUserIdsAsync({PlayerId}) end)
			if Success then
				for _,c in ipairs(UserData) do
					Name = c.DisplayName
					v.Name = Name
					v.Text = Name
				end
			end
		end
	end
else
	Username.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
	DisplayName.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
	for i,v in pairs(TargetsFrame:GetChildren()) do
		if v:IsA('TextButton') then
			PlayerId = v:FindFirstChildOfClass('NumberValue').Value
			local Success, UserData = pcall(function() return Services.UserService:GetUserInfosByUserIdsAsync({PlayerId}) end)
			if Success then
				for _,c in ipairs(UserData) do
					Name = c.Username
					v.Name = Name
					v.Text = Name
				end
			end
		end
	end
end

--TargetUser
Target.MouseButton1Click:Connect(function()
	local Text = TargetName.Text
	if Text == '' then
		return warn('Please Insert A Name')
	end
	for i,v in pairs(Services.Players:GetPlayers()) do
		if _G.Filter == 'Display' then
			Name = v.DisplayName
		else
			Name = v.Name
		end
		if string.lower(string.sub(Name,1,string.len(Text))) == string.lower(Text) then
			if v == LP then
				return warn("Can't Target Self")
			elseif table.find(_G.Targets,v.UserId) then
				return warn('This User Is Already Added')
			end
			TargetPlayer(v, TargetsFrame:FindFirstChild(Name))
			TargetName.Text = ''
			return warn('User: ' .. Name .. ' Has Been Added')
		end
	end
	return warn('User Not Found')
end)

--UnTargetUser
UnTarget.MouseButton1Click:Connect(function()
	local Text = TargetName.Text
	if Text == '' then
		return warn('Please Insert A Name')
	end
	for i,v in pairs(_G.Targets) do
		if _G.Filter == 'Display' then
			local Success, Data = pcall(function() return Services.UserService:GetUserInfosByUserIdsAsync({v}) end)
			if Success then
				for i,v in ipairs(Data) do
					Name = v.DisplayName
				end
			end
		else
			Name = Services.Players:GetNameFromUserIdAsync(v)
		end
		if string.lower(string.sub(Name,1,string.len(Text))) == string.lower(Text) then
			temptable = {
				Name = Services.Players:GetNameFromUserIdAsync(v),
				UserId = v
			}
			TargetPlayer(temptable, TargetsFrame:FindFirstChild(Name))
			TargetName.Text = ''
			return warn('User: ' .. Name .. ' Has Been Removed')
		end
	end
	return warn('User Not Found')
end)

--TargetOthers
All.MouseButton1Click:Connect(function()
	_G.TargetOthers = not _G.TargetOthers
	if _G.TargetOthers then
		All.Text = 'Target All: ON'
		for i,v in pairs(Services.Players:GetPlayers()) do
			if v ~= LP then
				table.insert(OthersTable, v.UserId)
			end
		end
		_G.OthersJoin = Services.Players.PlayerAdded:Connect(function(v)
			if not _G.TargetOthers then
				_G.OthersJoin:Disconnect()
			end
			table.insert(OthersTable, v.UserId)
		end)
	else
		All.Text = 'Target All: OFF'
		_G.OthersJoin:Disconnect()
		table.clear(OthersTable)
	end
end)

--Username
Username.MouseButton1Click:Connect(function()
	_G.Filter = 'User'
	Username.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
	DisplayName.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
	for i,v in pairs(TargetsFrame:GetChildren()) do
		if v:IsA('TextButton') then
			PlayerId = v:FindFirstChildOfClass('NumberValue').Value
			local Success, UserData = pcall(function() return Services.UserService:GetUserInfosByUserIdsAsync({PlayerId}) end)
			if Success then
				for _,c in ipairs(UserData) do
					Name = c.Username
					v.Name = Name
					v.Text = Name
				end
			end
		end
	end
end)

--DisplayName
DisplayName.MouseButton1Click:Connect(function()
	_G.Filter = 'Display'
	Username.BackgroundColor3 = Color3.fromRGB(3, 51, 132)
	DisplayName.BackgroundColor3 = Color3.fromRGB(16, 31, 66)
	for i,v in pairs(TargetsFrame:GetChildren()) do
		if v:IsA('TextButton') then
			PlayerId = v:FindFirstChildOfClass('NumberValue').Value
