			if Success then
				for _,c in ipairs(UserData) do
					Name = c.DisplayName
					v.Name = Name
					v.Text = Name
				end
			end
		end
	end
end)


--Diamond
Diamond.MouseButton1Click:Connect(function()
	_G.Diamond = not _G.Diamond
	if _G.Diamond then
		InsertGear(DiamondID, DiamondName)
		Diamond.Text = [[Diamond: 
ON]]
		while true do
			task.wait()
			if not _G.Diamond then
				break
			end
			pcall(function()
				for i,v in pairs(Services.Players:GetPlayers()) do
					if table.find(_G.Targets,v.UserId) or table.find(OthersTable,v.UserId) then
						if v.Character and v.Character:FindFirstChild('Humanoid') and v.Character:FindFirstChild('Humanoid').Health > 0 then
							DiamondFunction(v.Character.Humanoid, Damage)
						end
					end
				end
			end)
		end
	else
		Diamond.Text = [[Diamond: 
OFF]]
		_G.DiamondLoop:Disconnect()
	end
end)

--Rocket
Rocket.MouseButton1Click:Connect(function()
    _G.Rocket = not _G.Rocket
    if _G.Rocket then
        InsertGear(RocketID, RocketName)
        Rocket.Text = [[Rocket: 
ON]]
        while true do
        	task.wait()
            if not _G.Rocket then
                break
            end
            pcall(function()
                for i,v in pairs(Services.Players:GetPlayers()) do
                    if table.find(_G.Targets,v.UserId) or table.find(OthersTable,v.UserId) then
                        if v.Character and v.Character:FindFirstChild('Humanoid') and (v.Character:FindFirstChild('Humanoid').Health > 0 or v.Character:FindFirstChild('Humanoid').Health == 0/0) then
                            RocketFunction(Vector3.new(0,-9e9,0),v.Character.Head.Position)
                        elseif v.Character and not v.Character:FindFirstChild('Humanoid') then
                            RocketFunction(Vector3.new(0,-9e9,0),v.Character.Head.Position)
                        end
                    end
                end
            end)
        end
    else
        Rocket.Text = [[Rocket: 
OFF]]
    end
end)

--CBring
CBring.MouseButton1Click:Connect(function()
    _G.CBring = not _G.CBring
    if _G.CBring then
        CBring.Text = [[CBring: 
ON]]
        while true do
        	task.wait()
            if not _G.CBring then
                break
            end
            pcall(function()
                for i,v in pairs(Services.Players:GetPlayers()) do
                    if table.find(_G.Targets,v.UserId) or table.find(OthersTable,v.UserId) then
                        if v.Character and v.Character:FindFirstChild('Humanoid') then
                            CBringFunction(v.Character)
                        end
                    end
                end
            end)
        end
    else
        CBring.Text = [[CBring: 
OFF]]
    end
end)

_G.Unblind = LP.CharacterAdded:Connect(function()
	PlayerGui = LP:FindFirstChild('PlayerGui')
	for i,v in pairs(PlayerGui:GetChildren()) do
		if not table.find(SafeGuis, v.Name) then
			v:Destroy()
		end
	end
end)

if Services.CoreGui:FindFirstChild('PortGuiV1') then
	Services.CoreGui.PortGuiV1:Destroy()
end

PortGuiV1.Parent = Services.CoreGui
